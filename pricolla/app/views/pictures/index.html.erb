<p id="notice"><%= notice %></p>

<h1>Pictures</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pictures.each do |picture| %>
      <tr>
        <td><%= picture.name %></td>
        <td><%= picture.key %></td>
        <td><%= picture.temp_type %></td>
        <td><%= link_to 'Show', picture_path(picture, key: picture.key) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>